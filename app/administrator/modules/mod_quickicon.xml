	<author>Joomla! Project</author>
	<creationDate>January 2005</creationDate>
	<copyright>(C) 2005 Open Source Matters. All rights reserved.</copyright>
	<license>http://www.gnu.org/licenses/gpl-2.0.html GNU/GPL</license>
	<authorEmail>admin@joomla.org</authorEmail>
	<authorUrl>www.joomla.org</authorUrl>
	<version>1.0.0</version>
	<description>Модуль показывает кнопки быстрого доступа в админке панели управления</description>
	<files>
		<filename module="mod_quickicon">mod_quickicon.php</filename>
	</files>
	<params>
		<param name="securitycheck" type="radio" default="1" label="Проверка защиты" description="Показывать ли предупреждения системы защиты">
